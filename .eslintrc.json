{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
  "prettier",
  "plugin:prettier/recommended"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
  "prettier"
  ],
  "rules": {
      "indent": [
          "error",
          "tab"
      ],
  // "indent": "off",
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ],
      "react/prop-types": 0,
  "prettier/prettier": "error"
  }
}
